<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.netctoss.entity">
  <class name="Cost" table="Cost">
    <id name="id" column="ID" type="integer">
      <generator class="sequence">
        <param name="sequence">COST_SEQ</param>
      </generator>
    </id>
    <property name="name" column="NAME" type="string"></property>
    <property name="baseDuration" column="BASE_DURATION"></property>
    <property name="baseCost" column="BASE_COST"></property>
    <property name="unitCost" column="UNIT_COST"></property>
    <property name="status" column="STATUS"></property>
    <property name="descr" column="DESCR"></property>
    <property name="createTime" column="CREATIME"></property>
    <property name="startTime" column="STARTIME"></property>
    <property name="costType" column="COST_TYPE"></property>
  </class>

</hibernate-mapping>